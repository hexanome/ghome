protocole textuel, 

protocole d'envoi des id capteur de la base de données :
je demande les id = 
"GET_SENSORID
STOP
"
il me renvoit la liste d'ID =
"GIVE_SENSORID <XXXX> <XXXX> .... <XXXX>
STOP
"
protocole d'envoi des nouvelles données capteur :
"GIVE_SENSORSTATE  <sensorType> <sensorID> <value1> <value2> <value3>
STOP
"

structure de données capteur
enum sensorType {
    enOceanRockerSwitch = 1,
    enOceanTemperatureSensor = 2,
    enOceanSingleContact = 3,
    enOceanLightSensor = 4,
};

struct enOceanMessage
{
    enum sensorType type;
    char sensorID[4];
    float value1;
    float value2;
    float value3;    
};